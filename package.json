{
  "name": "conference-pdf-processor",
  "version": "1.0.0",
  "description": "A PDF processor for conferences.",
  "main": "./build_electron/electron/main.js",
  "scripts": {
    "init:app": "npm run init:electron && npm run init:frontend",
    "init:frontend": "cd frontend && npm install",
    "dev:frontend": "cd frontend && npm run dev",
    "build:frontend": "cd frontend && npm run build && npm run export",
    "start:frontend": "cd frontend && npm run start",
    "init:electron": "npm install",
    "start:electron": "tsc && electron ./build_electron/electron/main.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.yourcompany.yourAppId",
    "files": [
      "build_electron/**/*",
      "electron/**/*",
      "frontend/out/**/*"
    ],
    "win": {
      "target": "portable"
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "linux": {
      "target": [
        "AppImage",
        "snap"
      ]
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@next/font": "^13.4.19",
    "@pdf-lib/fontkit": "^1.1.1",
    "axios": "^1.5.0",
    "pdf-lib": "^1.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.11.0",
    "regedit": "^5.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.5.7",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "electron": "^26.1.0",
    "electron-builder": "^24.6.3",
    "typescript": "^5.2.2"
  }
}
